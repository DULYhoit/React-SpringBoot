<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.dao.UserDao">
  <insert id="insertMember" parameterType="com.app.entity.Register">
  	INSELT INTO MEMBER VALUES(#{regid}, #{regpw}, #{regemail}, #{regname},
  	  #{regbd}, ${regage}, #{regsex}, systimestamp, '010-1111-1111')
  </insert>
</mapper>